      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.grinCa.gestionchantier"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "scheme": "gestion-chantier"
  }
